server:
  port: 9070
  address: 0.0.0.0
  error:
    whitelabel:
      enabled: false
  servlet:
    session:
      cookie:
        same-site: strict

civil-registry-url: {{ civil_registry_url | default('http://auth-proxy:4180/fhir') }}

logging:
  file:
    name: oauth-route-middleware.log

dataSonnet:
  dhiS2Person: resource:file:/dhis2Person.ds

oauth2:
  tokenEndpoint: http://identity-provider:8080/realms/civil-registry/protocol/openid-connect/token
  clientId: civil-registry-client
  clientSecret: {{ clientSecret }}
